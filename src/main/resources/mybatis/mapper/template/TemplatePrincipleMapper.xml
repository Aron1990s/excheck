<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.aron.excheck.dao.template.TemplatePrincipleDao">

    <!-- 根据id获取全部模板规则 -->
    <select id="findRecordsById" parameterType="TemplatePrinciple" resultType="TemplatePrinciple">
        select * from template_principle where template_id = #{template_id}
    </select>

</mapper>
 
